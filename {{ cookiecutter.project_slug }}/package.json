    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "3.2.0",
    "tailwindcss": "4.1.4",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.24.2"
  },
  "devDependencies": {
